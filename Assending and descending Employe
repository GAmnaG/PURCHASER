Assending table:
CREATE TABLE Employe (
    EmployeeID INT PRIMARY KEY,
    FirstName VARCHAR(50),
    LastName VARCHAR(50),
    Department VARCHAR(50),
    Salary DECIMAL(10, 3)
);
INSERT INTO Employe (EmployeeID, FirstName, LastName, Department, Salary)
VALUES (1, 'Ubaid', 'yousaf', 'HR', 66000 );
INSERT INTO Employe (EmployeeID, FirstName, LastName, Department, Salary)
VALUES (2, 'Zayan', 'ali', 'CEO', 78000 );
INSERT INTO Employe(EmployeeID, FirstName, LastName, Department, Salary)
VALUES (3, 'Rema', 'asad', 'EMPLOYE', 42000 );
Dessending table:
CREATE TABLE Employe (
    EmployeeID INT PRIMARY KEY,
    FirstName VARCHAR(50),
    LastName VARCHAR(50),
    Department VARCHAR(50),
    Salary DECIMAL(10, 3)
);
INSERT INTO Employe (EmployeeID, FirstName, LastName, Department, Salary)
VALUES (1, 'Ubaid', 'yousaf', 'HR', 66000 );
INSERT INTO Employe (EmployeeID, FirstName, LastName, Department, Salary)
VALUES (2, 'Zayan', 'ali', 'CEO', 78000 );
INSERT INTO Employe(EmployeeID, FirstName, LastName, Department, Salary)
VALUES (3, 'Rema', 'asad', 'EMPLOYE', 42000 );
SELECT * FROM Employe
ORDER BY Salary DESC;
